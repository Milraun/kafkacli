{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "Node: Nodemon",
      "processId": "${command:PickProcess}",
      "restart": true,
      "protocol": "inspector"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl testProducer",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "--config=testConfig.json",
        "testProducer",
        "JayBeeTest",
        "-n",
        "100"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl listTopics",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "--config=config.json",
        "listTopics",
        ".*"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl describeTopicsConfig",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "describeTopicsConfig",
        "^VRZ-Measure.*"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl topicOffsets",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "--config=testConfig.json",
        "getTopicOffsets",
        "VRZ-ParaList"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl tailTopics follow",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "tailTopics",
        "^VRZ-SituationMerged",
        "--follow"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl tailTopics partitions",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "tailTopics",
        "^VRZ-SituationMerged",
        "--partitions=1,3"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafkacli tailTopics",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "--config=config.json",
        "tailTopics",
        "--json",
        "^CSM-UZ1-A23_1$"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl readYaml",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "readYaml"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl publish",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "publish",
        "external-situations.yaml"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl createTopic",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "createTopic",
        "hugo",
        "--numPartitions=2",
        "--replicationFactor=3"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl createTopics",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "createTopics",
        "createTopic.yaml"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl alterTopics",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "alterTopics",
        "createTopic.yaml"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "kafka-cl alterTopicsConfig",
      "program": "${workspaceFolder}\\src\\index.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [
        "alterTopicsConfig",
        "^hugo1$",
        "--configValue=cleanup.policy=compact",
        "--configValue=segment.ms=3600000"
      ],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Server",
      "program": "${workspaceFolder}/src/server.ts",
      "preLaunchTask": "tsc: build - tsconfig.json",
      "args": [],
      "outFiles": [
        "${workspaceFolder}/**/*.js"
      ],
      "outputCapture": "std"
    }
  ]
}